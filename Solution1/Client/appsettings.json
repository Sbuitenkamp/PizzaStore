{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5002"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "Data/Security/certificate.crt",
        "KeyPath": "Data/Security/private.key"
      }
    }
  }

}
